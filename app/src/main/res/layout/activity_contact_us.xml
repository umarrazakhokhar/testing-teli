<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ContactUsActivity"
    android:layout_centerHorizontal="true"
    android:layout_centerInParent="true"
    android:layout_centerVertical="true"
    android:background="@color/colorPrimary">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingTop="80dp"
        android:layout_margin="10dp"
        android:gravity="center">

        <ImageView
            android:id="@+id/wifi"
            android:layout_width="200dp"
            android:layout_height="200dp"
            app:srcCompat="@drawable/ashu"
            tools:ignore="MissingConstraints" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/wifi"
            android:text="Mohammad Ashfaque Khokhar"
            android:layout_marginTop="10dp"
            android:textColor="@color/Mybackground_color"
            android:fontFamily="sans-serif-black"
            android:textSize="21dp"
            android:textStyle="bold"
            android:textAlignment="center"
            android:layout_centerHorizontal="true"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/wifi"
            android:text="S/O Mohammad Ayyub Khokhar"
            android:layout_marginTop="10dp"
            android:textColor="@color/Mybackground_color"
            android:fontFamily="sans-serif-black"
            android:textSize="18dp"
            android:textStyle="bold"
            android:textAlignment="center"
            android:layout_centerHorizontal="true"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/wifi"
            android:text="Nagpur Maharashtra"
            android:layout_marginTop="10dp"
            android:textColor="@color/Mybackground_color"
            android:fontFamily="sans-serif-black"
            android:textSize="18dp"
            android:textStyle="bold"
            android:textAlignment="center"
            android:layout_centerHorizontal="true"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tableLayout"
            android:layout_gravity="start|bottom"
            android:gravity="bottom|center"
            android:orientation="horizontal"
            android:layout_marginTop="10dp">


            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/fab_Call"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="start|bottom"
                android:layout_margin="10dp"
                android:clickable="true"
                android:focusable="true"
                android:src="@android:drawable/stat_sys_phone_call"
                android:textAlignment="center"
                android:tint="@color/MyFabTint_color"
                app:backgroundTint="@color/MyFabBackground_color"
                app:fabSize="auto"
                app:maxImageSize="35dp" />

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/fab_Message"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end|bottom"
                android:layout_margin="10dp"
                android:clickable="true"
                android:focusable="true"
                android:tint="@color/MyFabTint_color"
                app:backgroundTint="@color/MyFabBackground_color"
                app:fabSize="normal"
                app:maxImageSize="30dp"
                app:srcCompat="@android:drawable/ic_dialog_email" />

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/fab_whatsapp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end|bottom"
                android:layout_margin="10dp"
                android:clickable="true"
                android:focusable="true"
                android:src="@drawable/whatsapp"
                android:tint="@color/MyFabTint_color"
                app:backgroundTint="@color/MyFabBackground_color"
                app:fabSize="normal"
                app:maxImageSize="60dp"/>

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/fab_admin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end|bottom"
                android:layout_margin="10dp"
                android:clickable="true"
                android:focusable="true"
                android:src="@android:drawable/ic_lock_idle_lock"
                android:tint="@color/MyFabTint_color"
                app:backgroundTint="@color/MyFabBackground_color"
                app:fabSize="normal"
                app:maxImageSize="40dp" />


        </LinearLayout>

    </LinearLayout>



</androidx.constraintlayout.widget.ConstraintLayout>